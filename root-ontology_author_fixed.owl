    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://example.org/chi2025#aboutTopic -->

    <owl:ObjectProperty rdf:about="http://example.org/chi2025#aboutTopic">
        <owl:inverseOf rdf:resource="http://example.org/chi2025#isTopicOf"/>
        <rdfs:domain rdf:resource="http://example.org/chi2025#Paper"/>
        <rdfs:range rdf:resource="http://example.org/chi2025#TopicConcept"/>
    </owl:ObjectProperty>
    


    <!-- http://example.org/chi2025#addressesIntention -->

    <owl:ObjectProperty rdf:about="http://example.org/chi2025#addressesIntention">
        <owl:inverseOf rdf:resource="http://example.org/chi2025#isAddressedBy"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://example.org/chi2025#SoftwareArtifact"/>
                    <rdf:Description rdf:about="http://example.org/chi2025#Study"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://example.org/chi2025#IntentionSpecification"/>
    </owl:ObjectProperty>
    


    <!-- http://example.org/chi2025#affiliatedWith -->

    <owl:ObjectProperty rdf:about="http://example.org/chi2025#affiliatedWith">
        <owl:inverseOf rdf:resource="http://example.org/chi2025#hasMember"/>
        <rdfs:domain rdf:resource="http://example.org/chi2025#Author"/>
        <rdfs:range rdf:resource="http://example.org/chi2025#Affiliation"/>
    </owl:ObjectProperty>
    


    <!-- http://example.org/chi2025#affiliation -->

    <owl:ObjectProperty rdf:about="http://example.org/chi2025#affiliation">
        <rdfs:domain rdf:resource="http://example.org/chi2025#Authorship"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/org#Organization"/>
        <rdfs:label>affiliation (organization @ time of paper)</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://example.org/chi2025#aimsAtGoal -->

    <owl:ObjectProperty rdf:about="http://example.org/chi2025#aimsAtGoal">
        <owl:inverseOf rdf:resource="http://example.org/chi2025#isGoalOf"/>
