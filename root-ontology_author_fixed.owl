    </owl:ObjectProperty>
    


    <!-- http://example.org/chi2025#hasAuthor -->

    <owl:ObjectProperty rdf:about="http://example.org/chi2025#hasAuthor">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/dc/terms/creator"/>
        <rdfs:domain rdf:resource="http://example.org/chi2025#Paper"/>
        <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://example.org/chi2025#hasAuthorship"/>
            <rdf:Description rdf:about="http://example.org/chi2025#author"/>
        </owl:propertyChainAxiom>
        <rdfs:label>has author (direct)</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://example.org/chi2025#hasAuthorOrg -->

    <owl:ObjectProperty rdf:about="http://example.org/chi2025#hasAuthorOrg">
        <rdfs:domain rdf:resource="http://example.org/chi2025#Paper"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/org#Organization"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://example.org/chi2025#hasAuthorship"/>
            <rdf:Description rdf:about="http://example.org/chi2025#affiliation"/>
        </owl:propertyChainAxiom>
        <rdfs:label>has author organization (inferred)</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://example.org/chi2025#hasAuthorship -->

    <owl:ObjectProperty rdf:about="http://example.org/chi2025#hasAuthorship">
