            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://example.org/chi2025#SoftwareArtifact"/>
                    <rdf:Description rdf:about="http://example.org/chi2025#Study"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://example.org/chi2025#GoalSpecification"/>
    </owl:ObjectProperty>
    


    <!-- http://example.org/chi2025#author -->

    <owl:ObjectProperty rdf:about="http://example.org/chi2025#author">
        <rdfs:domain rdf:resource="http://example.org/chi2025#Authorship"/>
        <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
