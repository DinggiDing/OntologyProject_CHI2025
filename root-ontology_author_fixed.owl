     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:bibo="http://purl.org/ontology/bibo/"
     xmlns:chi1="http://example.org/chi2025#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:hcio="urn:absolute:dev.nemo.inf.ufes.br/hcion/HCIO.owl#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:schema="http://schema.org/">
    <owl:Ontology rdf:about="http://example.org/chi2025">
        <owl:versionIRI rdf:resource="http://example.org/chi2025/1.0"/>
        <owl:imports rdf:resource="http://example.org/chi/instances"/> 
        <owl:imports rdf:resource="urn:absolute:dev.nemo.inf.ufes.br/hcion/HCIO.owl#/1.0"/>
        <rdfs:label>CHI 2025 Ontology</rdfs:label>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
